#include<stdio.h>
int main()
{
	int choice=0;
	printf("Pick your food item : \n1. Pizza \n2. Burger \n3. Pasta \n4. French fries \n 5. Sandwich");
	printf("\nenter your choice :");
	scanf("%d",&choice);
	switch(choice)
	{
		case 1:
			printf("Food item - Pizza");
			printf("\nPrice - Rs 239");
			break;
			case 2:
				  printf("Food item - Burger");
			    printf("\nPrice - Rs 129");
			    break;
			    case 3:
				      printf("Food item - Pasta");
			        printf("\nPrice - Rs 179");
			        break;
			        case 4:
				          printf("Food item - French fries");
			            printf("\nPrice - Rs 99");
			            break;
			             case 5:
				                printf("Food item - Sandwich");
			                  printf("\nPrice - Rs 149");
			                  break;
			                  default :
			                  	printf("enter valid choice:");
			
	}
	return 0;
}
